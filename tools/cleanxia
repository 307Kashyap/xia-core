#!/bin/sh
# clean all of the XIA components

XIADIR="$(dirname "$(cd "$(dirname "$0")" && pwd)")" 

cd $XIADIR/click
make clean

cd $XIADIR/XIASocket/API
make clean

cd $XIADIR/XIASocket/sample
make clean

cd $XIADIR/example
make clean

cd $XIADIR/daemons/xroute
make clean

cd $XIADIR/daemons/xhcp
make clean

cd $XIADIR/daemons/nameserver
make clean

cd $XIADIR
